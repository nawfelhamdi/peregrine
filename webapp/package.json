{
  "name": "peregrine",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc && npm install --prefix client && npm run build --prefix client",
    "server": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "client": "cd client && npm start && cd ..",
    "dev": "concurrently \"npm run client\" \"npm run server\""
  },
  "dependencies": {
    "@azure/storage-blob": "^12.10.0",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "helmet": "^5.1.0",
    "morgan": "~1.9.1",
    "mssql": "^6.4.1",
    "reflect-metadata": "^0.1.13",
    "tedious": "^14.5.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/mssql": "^8.0.2",
    "@types/node": "^17.0.42",
    "@types/validator": "^13.7.3",
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.3"
  }
}
