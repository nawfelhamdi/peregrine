										},
										"File": {
											"value": "@item().source_table_name",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "csv_file",
									"type": "DatasetReference",
									"parameters": {
										"Container": {
											"value": "@concat(item().target_table_schema,'/',pipeline().parameters.project_id)",
											"type": "Expression"
										},
										"File": {
											"value": "@item().target_table_name",
											"type": "Expression"
										}
									}
								}
							]
						},
						{
							"name": "Delete",
							"type": "Delete",
							"dependsOn": [
								{
									"activity": "copy to archive folder",
									"dependencyConditions": [
										"Succeeded"
									]
