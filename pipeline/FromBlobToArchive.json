							},
							"userProperties": [],
							"typeProperties": {
								"dataset": {
									"referenceName": "csv_file",
									"type": "DatasetReference",
									"parameters": {
										"Container": {
											"value": "@item().source_table_schema",
											"type": "Expression"
										},
										"File": {
											"value": "@item().source_table_name",
											"type": "Expression"
										}
									}
								},
								"enableLogging": false,
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								}
							}
						}
					]
				}
